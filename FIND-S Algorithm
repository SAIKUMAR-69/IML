import pandas as pd
import numpy as np

data = pd.read_csv('dataset (1).csv')

print(data.head())  

print("Unique values in the target column:", data.iloc[:, -1].unique())  

concept = np.array(data)[:, :-1]  
target = np.array(data)[:, -1]  

def train(con, tar):
    specific_h = None  
    
    for i, val in enumerate(tar):
        if val == "True":
            specific_h = con[i].copy()  
            break  
    
    
    if specific_h is None:
        print("No positive ('True') instances found in the target.")
        return None  
    
    print("Initial specific_h:", specific_h)  
    
    
    for i, val in enumerate(con):
        if tar[i] == "True":
            for x in range(len(specific_h)):
                if val[x] != specific_h[x]:
                    specific_h[x] = '?'  
    
    return specific_h


result = train(concept, target)
if result is not None:
    print("Final hypothesis:", result)
